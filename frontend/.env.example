# Frontend environment variables example
# Copy this file to .env and fill in the values

# API URL for backend (used by Vite proxy in development)
VITE_API_URL=http://localhost:3000

# Public API URL (accessible in browser)
# In development, leave empty to use proxy
# In production, set to your deployed backend URL (e.g., https://your-backend.railway.app)
PUBLIC_API_URL=
